<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.aplikacjabankowajava.transactionPropertiesController">
    <children>
        <Button fx:id="back" mnemonicParsing="false" onAction="#goBack" text="Powrót" />
        <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label fx:id="transactionNumberText" layoutX="14.0" layoutY="14.0" text="Transakcja ...">
               <font>
                  <Font size="32.0" />
               </font>
            </Label>
            <Label fx:id="titleText" layoutX="14.0" layoutY="57.0" text="Tytuł transakcji ...">
               <font>
                  <Font size="20.0" />
               </font></Label>
            <Label fx:id="amountText" layoutX="14.0" layoutY="121.0" text="Kwota operacji">
               <font>
                  <Font size="20.0" />
               </font></Label>
            <Label fx:id="balanceLabel" layoutX="11.0" layoutY="189.0" text="Saldo po operacji .. ?">
               <font>
                  <Font size="20.0" />
               </font></Label>
            <Label fx:id="dateText" layoutX="14.0" layoutY="268.0" text="Data operacji">
               <font>
                  <Font size="20.0" />
               </font></Label>
            <Label fx:id="senderName" layoutX="301.0" layoutY="61.0" text="Nadawca">
               <font>
                  <Font size="20.0" />
               </font></Label>
            <Label fx:id="receiverName" layoutX="300.0" layoutY="189.0" text="Odbiorca">
               <font>
                  <Font size="20.0" />
               </font></Label>
            <Label fx:id="senderAcc" layoutX="301.0" layoutY="121.0" text="Nr konta nadawcy">
               <font>
                  <Font size="20.0" />
               </font></Label>
            <Label fx:id="receiverAcc" layoutX="300.0" layoutY="268.0" text="Nr konta odbiorcy">
               <font>
                  <Font size="20.0" />
               </font></Label>
            <Button layoutX="396.0" layoutY="361.0" mnemonicParsing="false" onAction="#generate" text="Generuj potwierdzenie transakcji" />
            <Label fx:id="typeLabel" layoutX="14.0" layoutY="329.0" text="Typ transakcji">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
         </children>
        </AnchorPane>
    </children>
</VBox>
